#include<stdio.h>
int main()
{
    int arr[100]={23,56,22,86,35,76};
    int n=6,i,temp;
    for(i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
    printf("\n");
    for(i=0;i<n/2;i++)
    {
    if(arr[i]==arr[n-i-1])
    {
        printf("%d ",arr[i]);
    }
    else                                                 
    {
        temp=arr[i];
        arr[i]=arr[n-i-1];
        arr[n-i-1]=temp;
        
    }
    }
    for(i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
    return 0;
}


//Alternate and simple method.





#include<stdio.h>
int main()
{
    int arr[100]={23,56,22,86,35,76};
    int n=6,i;
    for(i=0;i<n;i++)
    {
        printf("%d ",arr[i]);
    }
    printf("\n");
    
    return 0;
}
