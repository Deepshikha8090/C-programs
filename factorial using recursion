#include <stdio.h>
int factorial(int);

int main()
{
    int n;
    printf("Enter the number you want to calculate factorial:");
    scanf("%d",&n);
    printf("the factorial of the given number is %d",factorial(n));
    return(0);
}

    int factorial(int n){
        int i;{
            if(n==0||n==1)
            return 1;
            else
            return n*factorial(n-1);
        }
        }
